{
  "name": "contrastream",
  "version": "0.0.2",
  "description": "A specialized static site generator for building websites for businesses/projects. Compose sites using a next-gen toolkit, a component-based architecture, and markdown/TOML content. Built-in support for blogs/docs sections.",
  "main": "contra.js",
  "bin": {
    "website": "build.js"
  },
  "scripts": {
    "dev": "npm run build:watch && npm run server && npm run test:watch",
    "build": "make",
    "build:watch": "watch 'make' src",
    "test": "mocha -R spec --compilers js:babel/register ./test/endpoint.js",
    "test:watch": "npm run test -- -w",
    "server": "./node_modules/babel/bin/babel-node.js tools/server.js",
    "repl": "./node_modules/babel/bin/babel-node.js"
  },
  "author": "",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "basscss-align": "^1.0.0",
    "basscss-background-colors": "^1.1.1",
    "basscss-base-forms": "^2.0.0",
    "basscss-base-reset": "^2.0.2",
    "basscss-base-tables": "^1.0.0",
    "basscss-base-typography": "^2.0.1",
    "basscss-borders": "^2.0.4",
    "basscss-btn": "^1.0.1",
    "basscss-btn-outline": "^1.0.0",
    "basscss-btn-primary": "^1.0.0",
    "basscss-color-base": "^2.0.0",
    "basscss-color-forms": "^3.0.0",
    "basscss-color-tables": "^1.0.2",
    "basscss-colors": "^2.0.0",
    "basscss-grid": "^1.0.3",
    "basscss-positions": "^1.0.4",
    "basscss-responsive-states": "^1.0.3",
    "basscss-type-scale": "^1.0.0",
    "basscss-utility-layout": "^2.0.3",
    "basscss-utility-typography": "^2.0.0",
    "basscss-white-space": "^1.1.2",
    "flex-object": "^2.0.2"
  },
  "devDependencies": {
    "babel": "<6.0.0",
    "babel-core": "<6.0.0",
    "babel-eslint": "^4.1.3",
    "babel-plugin-closure-elimination": "0.0.2",
    "babel-plugin-typecheck": "^2.0.0",
    "baobab": "^2.1.0",
    "chokidar": "^1.2.0",
    "connect": "^3.4.0",
    "cssnext": "^1.8.4",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-babel": "^2.1.1",
    "fs-extra": "^0.25.0",
    "glob": "^5.0.15",
    "logdown": "^1.2.4",
    "marked": "^0.3.5",
    "mocha": "^2.3.3",
    "mz": "^2.1.0",
    "nunjucks": "^2.1.0",
    "nunjucks-markdown": "^1.1.0",
    "rimraf": "^2.4.3",
    "superstatic": "^3.0.0",
    "taim": "^1.0.2",
    "toml": "^2.3.0",
    "watch": "^0.16.0"
  }
}
